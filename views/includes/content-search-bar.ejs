<section class="section-search">
  <form
    class="form-word-search"
    action="/search"
    method="POST"
    role="search window"
    aria-label="word search window"
    id="search-form"
    autocomplete="off"
  >
    <div class="form__search">
      <div class="search-container">
        <label for="search-box"></label>
        <input
          class="search-input"
          type="text"
          id="search-input"
          placeholder="Enter word here"
          title=" Search may be performed in English or any Japanese alphabet. You may
        include a (*ï¼Š) in front or behind letters to perform a wildcard search."
        />
        <ul
          id="live-search"
          class="search-predictions"
          role="search predictions"
          aria-label="list of words in database matching input that user may be searching for"
        ></ul>
      </div>
      <button class="search__btn" type="submit">Search</button>
    </div>
  </form>
</section>
